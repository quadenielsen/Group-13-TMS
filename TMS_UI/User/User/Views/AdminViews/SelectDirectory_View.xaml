<UserControl x:Class="User.Views.AdminViews.SelectDirectory_View"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:User.Views.AdminViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Content="SELECT DIRECTORY (ADMIN)"/>
        <Button Content="go to ADMIN HOME" Command="{Binding NavigateHomeCommand}" Margin="100"/>
        <Button Content="go to login" Command="{Binding NavigateLoginCommand}" Margin="100" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
