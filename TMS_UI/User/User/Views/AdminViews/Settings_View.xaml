<UserControl x:Class="User.Views.AdminViews.Settings_View"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:User.Views.AdminViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Content="ADMIN SETTINGS"/>
        <Button Content="go to admin home" Command="{Binding NavigateHomeCommand}" Margin="200"/>
        <Button Content="go to login screen" Command="{Binding NavigateLoginCommand}" Margin="50" VerticalAlignment="Top"/>
        <Button Content="go to select directory" Command="{Binding NavigateSelectDirectoryCommand}" Margin="50" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
